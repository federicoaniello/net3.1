@model List<Brand>




<main id="brands-carousel">
    <h3 class="text-center py-5 pb-4">Rivenditore Ufficiale di</h3>
    <div class="container-md pb-5">
    <div class=" px-1 swiper brands-list">

        <div class="brands swiper-wrapper">
            @foreach (Brand prod in Model)
            {
                <div class="product swiper-slide">
                    <img class="swiper-lazy" data-src="/images/brands/@(prod.Img)">
                </div>
            }
        </div>
        
    </div>
    </div>
</main>
